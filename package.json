{
  "name": "@edgehub/router",
  "version": "0.0.16",
  "description": "A minimal Express.js like router for cloudflare workers.",
  "keywords": [],
  "homepage": "https://github.com/edge-hub/router#readme",
  "bugs": {
    "url": "https://github.com/edge-hub/router/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edge-hub/router.git"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "author": "vinaypuppal",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && yarn rollup -c rollup.config.js",
    "prepublish": "yarn build && cp package.json dist/package.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.md": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.0.9",
    "@types/cookie": "^0.3.3",
    "@types/trouter": "^3.1.0",
    "cookie": "^0.4.0",
    "trouter": "^3.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.2.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "typescript": "^3.8.3"
  },
  "filesize": {
    "track": [
      "./dist/index.umd.js"
    ]
  }
}
